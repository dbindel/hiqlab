#===============================================================================
# ILP32 mode, Goto BLAS, do not test for integer overflow. No divide by zero.
#===============================================================================

CC = gcc
CFLAGS = -O0 -ftest-coverage -fprofile-arcs -O0
UMFPACK_CONFIG = -DTESTING -DNO_DIVIDE_BY_ZERO
LIB = -lgoto -lxerbla -lfrtbegin -lg2c -lm

RANLIB = ranlib
MV = mv -f
RM = rm -f
MEX = mex -inline -g
AR = ar cr

#===============================================================================

CLEAN = *.o *.obj *.ln *.bb *.bbg *.da *.c.tcov *.c.gcov gmon.out
